// Look for the closest option.
shopdirect.event.subscribe('m2m/checkMeasurements', function (self) {
var prodId = document.getElementsByName('id')[0].value || undefined;
var chosenCombination = false;
var chosenSku;
var width;
var drop;
var subset;
var i;
var j;
m2m.widthMeasurement = parseInt(self.find('.widthMeasurementInput').val()) || parseInt($('.widthMeasurementInput').val()) || 0;
m2m.dropMeasurement = parseInt(self.find('.dropMeasurementInput').val()) || parseInt($('.dropMeasurementInput').val()) || 0;
// Convert inches to cm.
if (m2m.unit === 'inch') {
m2m.widthMeasurement *= 2.54;
m2m.dropMeasurement *= 2.54;
}
m2m.widthMeasurement = Math.round(m2m.widthMeasurement * 100) / 100;
m2m.dropMeasurement = Math.round(m2m.dropMeasurement * 100) / 100;
// Validate input
if (m2m.widthMeasurement >= m2m.minWidth &&
m2m.widthMeasurement <= m2m.maxWidth &&
m2m.dropMeasurement >= m2m.minDrop &&
m2m.dropMeasurement <= m2m.maxDrop
) {
$('.validMeasurement').prop('checked', true);
} else {
// Reset fields to prevent wrong size being ordered.
$('.widthDropdown, .dropDropdown').find('input:checked').prop('checked', false);
$('.validMeasurement').prop('checked', false);
$('.stockMessaging').hide();
sdg.productOptions[prodId].chosen[parseInt($('.widthDropdown').attr('rel'), 10) - 1] = null;
sdg.productOptions[prodId].chosen[parseInt($('.dropDropdown').attr('rel'), 10) - 1] = null;
sdg.productOptions.optionChange(prodId, false);
return;
}
// Calculate closest combination of width and drop.
for (i = 0; i < combinations.length; i += 1) {
if (!chosenCombination) {
width = combSplit(combinations[i], 0);
subset = $.grep(combinations, function (combo) {
return combSplit(combo, 0) === width;
});
if (m2m.widthMeasurement <= width) {
for (j = 0; j < subset.length; j += 1) {
drop = combSplit(subset[j], 1);
if (m2m.dropMeasurement <= drop) {
chosenCombination = subset[j];
break;
}
}
}
} else {
break;
}
}
