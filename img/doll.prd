return false;
});
// After entering value in width/drop inputs.
$('body').on('keydown', '.widthMeasurementInput, .dropMeasurementInput', function (e) {
return m2m.isNumeric(e);
});
$('body').on('keyup', '.widthMeasurementInput, .dropMeasurementInput', function (e) {
var $this = $(this);
var num = $this.val();
if (m2m.isNumeric(e)) {
