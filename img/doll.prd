var bWidth = combSplit(b, 0);
var bDrop = combSplit(b, 1);
if (aWidth < bWidth) { return -1; }
if (aWidth > bWidth) { return 1; }
if (aWidth === bWidth) {
if (aDrop < bDrop) { return -1; }
if (aDrop > bDrop) { return 1; }
return 0;
}
});
// Store maximum values for validation.
m2m.maxWidth = combSplit(combinations[combinations.length - 1], 0);
m2m.maxDrop = combSplit(combinations[combinations.length - 1], 1);
// Hide current UI.
$options = $('#main').find('.productOptionItem').not('.colourSwatches, .itemQuantity').hide();
// Hide dialog box validation title.
