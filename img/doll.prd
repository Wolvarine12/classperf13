<script type="text/javascript">
//VANILLA JS DOM READY FUNCTION - IE9+
const domReady = function (callback) {
// Stole from http://goo.gl/oy0itN
if (document.readyState === 'interactive' || document.readyState === 'complete') {
callback();
} else {
document.addEventListener('DOMContentLoaded', callback);
}
};
function match(option) {
let text = null;
['ipad', 'Ipad', 'IPAD', 'iPad'].forEach(function (item) {
if (option.includes(item)) {
return text = item;
}
});
return text;
}
function getSelectorQuery () {
let selector = null;
const isDesktop = window.sdgGA && sdgGA.device === 'desktop';
const brand = window.universal_variable && universal_variable.page.site;
const genericAppleOption = 'OPTION-Apple';
const iPadAirOption = 'OPTION-iPad Air';
const isiPadAir = isDesktop && document.querySelector(`.productOptionsList .productOptionItem.skuAttribute [for*="${iPadAirOption}"]`);
if (brand === 'very') {
selector = isDesktop ? `.productOptionsList .productOptionItem.skuAttribute .ppOption .ppOption__item [for*="${isiPadAir ? iPadAirOption : genericAppleOption}"] span` : ".productOptions #ppOption .ppOption__section .ppOption__item";
} else if (brand === 'littlewoods') {
selector = isDesktop ? `.productOptionsList .productOptionItem.skuAttribute .customerSelection [for*="${isiPadAir ? iPadAirOption : genericAppleOption}"] span` : ".productOptions .productOptionsList .productOptionItem.skuAttribute .holder .sel-holder select[id*='productOption'] option";
} else if (brand === 'lwi') {
selector = isDesktop ? `.productOptionsList .productOptionItem.skuAttribute fieldset ul.customerSelection [for*="${isiPadAir ? iPadAirOption : genericAppleOption}"] span` : ".productOptions .productOptionsList .productOptionItem.skuAttribute .holder .sel-holder select[id*='productOption'] option";
}
return selector;
}
domReady(function () {
const options = document.querySelectorAll(getSelectorQuery());
if (options && options.length) {
options.forEach(function (el) {
const text = el.innerHTML;
el.innerHTML = text.replace(match(text), "<span class='aie_396 iPad_text'>iPad</span>");
});
}
});
</script>
